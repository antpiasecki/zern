func main[] : i64
    let sum_of_squares: i64 = 0
    for i in 1..101
        sum_of_squares = sum_of_squares + i * i

    let square_of_sum: i64 = 0
    for i in 1..101
        square_of_sum = square_of_sum + i
    square_of_sum = square_of_sum * square_of_sum

    io.println_i64(square_of_sum - sum_of_squares)