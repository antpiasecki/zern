func main[] : I64
    let out: I64 = 0

    for a in 500..1000
        for b in 500..1000
            if a * b > out
                let s: String = str.from_i64(a * b)
                let s_rev: String = str.reverse(s)
                if c.strcmp(s, s_rev) == 0
                    out = a * b
                c.free(s)
                c.free(s_rev)
    io.print_i64(out)