func main[] : i64
    let answer: i64 = math.abs(os.urandom_i64()) % 100

    while true
        io.println("Guess a number: ")
        let guess: i64 = io.read_line() |> str.trim() |> str.parse_i64()

        if guess == answer
            io.println("You win!")
            break
        else if guess < answer
            io.println("Too low!")
        else
            io.println("Too high!")